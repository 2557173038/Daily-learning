<!--pages/7-swiper/7-swiper.wxml-->
<button type="primary" bindtap="handleAjaxSwper">ajax-swiper</button>
<swiper indicator-dots="{{true}}" circular="{{true}}" interval="{{2000}}" 	        autoplay="{{true}}">
  <swiper-item  wx:for="{{datalist}}" wx:key="index">
  <image src="{{item.image_url}}" mode="widthFix"/>
  </swiper-item>
</swiper>